#include <stdio.h>
#include<string.h>
#include<ctype.h>
int main(){
    char str[100];
    printf("enter string:");
    fgets(str,sizeof(str),stdin);
    int i,j=0;
    for(i=0;i<strlen(str);i++){
        if(tolower(str[i])!='a' && tolower(str[i])!='e' && tolower(str[i])!='i' && tolower(str[i])!='o' && tolower(str[i])!='u'){
            str[j]=str[i];
            j++;
        }
    }
    str[j]='\0';
    printf("String after removing all thhe vowels is %s",str);
    return 0;
}
