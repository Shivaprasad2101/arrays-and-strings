
#include <stdio.h>
#include<string.h>
int main() {
    // Write C code here
     char str[100]="hello world";
    int i,j=0;
    for(i=0;i<strlen(str);i++){
        if(str[i]!=' '){
            str[j]=str[i];
            j++;
        }
    }
    str[j]='\0';
    printf("String w/o space is %s",str);
    return 0;
}
